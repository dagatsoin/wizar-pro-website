{
  "name": "gatsby-theme-netlifycms",
  "description": "A Gatsby theme for Netlify CMS with working in monorepo.",
  "version": "0.0.1",
  "author": "Daniel Neveux <daniel.neveux@gmail.com>",
  "dependencies": {
    "gatsby": "^2.15.28",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-remark": "^2.3.12",
    "netlify-cms-app": "^2.9.7",
    "read-yaml-promise": "^1.0.2",
    "slash": "^3.0.0",
    "unist-util-select": "^3.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "npm run clean && gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "node_modules/typescript/bin/tsc --noEmit",
    "lint": "tslint --project ."
  },
  "devDependencies": {
    "gatsby-plugin-netlify-cms": "^4.1.21",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "prettier": "^1.18.2"
  }
}
